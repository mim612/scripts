function param_maker
    set input_source $argv[1]
    set value $argv[2]

    set counter 0
    set query_string "?"

    if test "$input_source" = "-"
        set lines (cat)
    else
        set lines (cat "$input_source")
    end

    for keyword in $lines
        if test -n "$keyword"
            set counter (math "$counter + 1")
            set query_string "$query_string$keyword=${value}$counter&"
        end

        if test $counter -eq 25
            echo (string trim -r -c '&' -- "$query_string")
            set query_string "?"
            set counter 0
        end
    end

    if test $counter -gt 0
        echo (string trim -r -c '&' -- "$query_string")
    end
end
