let xhr = new XMLHttpRequest();
xhr.open("GET", "https://memoryleaks.ir/me");
xhr.onreadystatechange = function () {
  if (xhr.readyState === 4) {
    if (xhr.status === 200) {
      console.log("Response:", xhr.responseText);
    } else {
      console.error("Error:", xhr.status);
    }
  }
};
xhr.send();
